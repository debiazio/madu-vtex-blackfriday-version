  "flex-layout.row#shop-by-category": {
    "props": {
      "blockClass": "linha-compre-por-categorias",
      "preventHorizontalStretch": true
    },
    "children": [
      "flex-layout.row#linha-titulo-categoria",
      "flex-layout.row#linha-colunas-categorias"

    ]
  },
  "flex-layout.row#linha-titulo-categoria": {
    "props": {
      "blockClass": "linha-titulo-categoria"
    },
    "children": [
      "rich-text#titulo-shop-by-categories"
    ]
  },
"flex-layout.row#linha-colunas-categorias":{
  "props": {
    "blockClass": "linha-colunas-categoria"
  },
  "children": [
      "flex-layout.col#shopby-carrinhos",
      "flex-layout.col#shopby-armarios",
      "flex-layout.col#shopby-estantes",
      "flex-layout.col#shopby-organizadores",
      "flex-layout.col#shopby-lancamentos"

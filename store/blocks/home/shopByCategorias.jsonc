  // componente do slider
  "slider-layout#shopByCategorias":{
    "props": {
      "title": "Slider shopByCategorias",
      "blockClass": "slider-shopByCategorias",
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "arrowSize": 25,
      "infinite": true,
      "showNavigationArrows": "always",
      "showPaginationDots": "always",
      "autoplay": {
        "timeout": 8000,
        "stopOnHover": true
      }
    }
  },
